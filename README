
::

  $ alias mishmash='eyeD3 -P mishmash --db-type postgres'

config.ini::

  [mishmash]
  # this does not yet work (is not passed), currently have in an alias
  options = --db-type=postgres --database=mishmash --password=travis

  $ alias mishmash='eyeD3 --plugin=mishmash'

postgresql
----------
::
  
  $ su - postgres
  $ createuser -s travis
  $ exit
  $ createdb -U travis -T template0 -E UTF8 mishmash.db 
