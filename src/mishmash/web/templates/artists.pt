<metal:block use-macro="main_template">
  <div metal:fill-slot="content">

<div align="center">
  <span tal:repeat="L artist_keys"><a href="#${L}">${L}</a>&nbsp;</span>
</div>

<span tal:repeat="L artist_keys"><a id='${L}'><strong>${L}</strong></a>
  <ul>
    <li tal:repeat="artist artist_dict[L]">
      <a href="${request.route_url('single_artist', name=artist.url_name)}"
         tal:content="artist.sort_name">Artist</a>
    </li>
  </ul>
</span>

  </div>
</metal:block>
